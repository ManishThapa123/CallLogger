<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/onboarding_navigation"
    app:startDestination="@id/permissionFragmentFirst">

    <fragment
        android:id="@+id/permissionFragmentFirst"
        android:name="com.eazybe.callLogger.ui.Onboarding.PermissionFragmentFirst"
        android:label="fragment_permission_first"
        tools:layout="@layout/fragment_permission_first" >
        <action
            android:id="@+id/action_permissionFragmentFirst_to_permissionFragmentSecond"
            app:destination="@id/permissionFragmentSecond"
            app:enterAnim="@anim/slide_in_from_right_to_left"
            app:exitAnim="@anim/slide_out_from_right_to_left"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/permissionFragmentFirst"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/permissionFragmentSecond"
        android:name="com.eazybe.callLogger.ui.Onboarding.PermissionFragmentSecond"
        android:label="fragment_permission_second"
        tools:layout="@layout/fragment_permission_second" />
</navigation>