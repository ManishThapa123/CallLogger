<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:fillViewport="true"
    android:scrollbars="none"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    tools:context=".ui.Dashboard.ReportsFragment">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="visible"
        android:id="@+id/lLParent"
        android:orientation="vertical">
        <ProgressBar
            android:id="@+id/pbProgress"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:progressTint="@color/purple_500"
            android:visibility="gone"
            android:layout_gravity="center"/>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:id="@+id/clDashBoard"
            android:visibility="gone"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/header_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:visibility="gone"
                android:layout_marginTop="@dimen/dimens_20"
                android:fontFamily="@font/poppins_semibold"
                android:text="Dashboard"
                android:textColor="@color/blackish"
                android:textSize="@dimen/dimens_18"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/eazybe_link"
                android:textSize="@dimen/dimens_12"
                android:layout_marginStart="@dimen/dimens_12"
                android:layout_marginEnd="@dimen/dimens_12"
                android:gravity="center"
                android:fontFamily="@font/poppins"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/header_title"
                android:id="@+id/workspaceText"/>

            <TextView
                android:id="@+id/callSummaryTxt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dimens_20"
                android:layout_marginTop="@dimen/dimens_12"
                android:fontFamily="@font/poppins_medium"
                android:text="Call Summary"
                android:textColor="@color/grayish"
                android:textSize="@dimen/dimens_16"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/workspaceText" />

            <Spinner
                android:id="@+id/dateDropDown"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dimens_12"
                android:layout_marginEnd="@dimen/dimens_20"
                app:layout_constraintBottom_toBottomOf="@+id/callSummaryTxt"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/callSummaryTxt" />



            <LinearLayout
                android:id="@+id/linearLayoutSet1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dimens_16"
                android:orientation="horizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/dateDropDown">

                <androidx.cardview.widget.CardView
                    android:layout_width="0dp"
                    android:layout_height="140dp"
                    android:layout_marginStart="@dimen/dimens_16"
                    android:layout_marginEnd="@dimen/dimens_12"
                    android:layout_marginBottom="@dimen/dimens_2"
                    android:layout_weight="1"
                    android:elevation="@dimen/dimens_32"
                    app:cardCornerRadius="@dimen/dimens_16">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="@dimen/dimens_24"
                                android:layout_height="@dimen/dimens_24"
                                android:layout_marginStart="@dimen/dimens_24"
                                android:layout_marginEnd="@dimen/dimens_8"
                                android:src="@drawable/ic_incoming_dashboard" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/poppins_medium"
                                android:text="Incoming"
                                android:textColor="@color/blackish"
                                android:textSize="@dimen/dimens_14" />
                        </LinearLayout>

                        <com.robinhood.ticker.TickerView
                            android:id="@+id/totalIncomingCalls"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="start"
                            android:layout_marginStart="@dimen/dimens_24"
                            android:layout_marginTop="@dimen/dimens_10"
                            android:layout_marginBottom="@dimen/dimens_8"
                            android:text="0"
                            android:textColor="@color/black"
                            android:textSize="@dimen/dimens_24"
                            app:ticker_animationDuration="700"
                            app:ticker_defaultPreferredScrollingDirection="up" />


                        <com.robinhood.ticker.TickerView
                            android:id="@+id/totalIncomingDuration"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="start"
                            android:layout_marginStart="@dimen/dimens_24"
                            android:layout_marginBottom="@dimen/dimens_4"
                            android:text="12 hrs 41 mins"
                            android:textColor="#4F71B1"
                            android:textSize="@dimen/dimens_12"
                            app:ticker_animationDuration="700"
                            app:ticker_defaultPreferredScrollingDirection="up" />

                    </LinearLayout>


                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="0dp"
                    android:layout_height="140dp"
                    android:layout_marginEnd="@dimen/dimens_16"
                    android:layout_marginBottom="@dimen/dimens_2"
                    android:layout_weight="1"
                    android:elevation="@dimen/dimens_20"
                    app:cardCornerRadius="@dimen/dimens_16">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="@dimen/dimens_24"
                                android:layout_height="@dimen/dimens_24"
                                android:layout_marginStart="@dimen/dimens_28"
                                android:layout_marginEnd="@dimen/dimens_8"
                                android:src="@drawable/ic_outgoing_dashboard" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/poppins_medium"
                                android:text="Outgoing"
                                android:textColor="@color/blackish"
                                android:textSize="@dimen/dimens_14" />
                        </LinearLayout>

                        <com.robinhood.ticker.TickerView
                            android:id="@+id/totalOutgoingCalls"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="start"
                            android:layout_marginStart="@dimen/dimens_24"
                            android:layout_marginTop="@dimen/dimens_10"
                            android:layout_marginBottom="@dimen/dimens_8"
                            android:text="0"
                            android:textColor="@color/black"
                            android:textSize="@dimen/dimens_24"
                            app:ticker_animationDuration="700"
                            app:ticker_defaultPreferredScrollingDirection="up" />


                        <com.robinhood.ticker.TickerView
                            android:id="@+id/totalOutgoingDuration"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="start"
                            android:layout_marginStart="@dimen/dimens_24"
                            android:layout_marginBottom="@dimen/dimens_4"
                            android:text="12 hrs 41mins"
                            android:textColor="#4F71B1"
                            android:textSize="@dimen/dimens_12"
                            app:ticker_animationDuration="700"
                            app:ticker_defaultPreferredScrollingDirection="up" />

                    </LinearLayout>


                </androidx.cardview.widget.CardView>


            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayoutSet2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dimens_8"
                android:orientation="horizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/linearLayoutSet1">

                <androidx.cardview.widget.CardView
                    android:layout_width="0dp"
                    android:layout_height="140dp"
                    android:layout_marginStart="@dimen/dimens_16"
                    android:layout_marginEnd="@dimen/dimens_12"
                    android:layout_marginBottom="@dimen/dimens_2"
                    android:layout_weight="1"
                    android:elevation="@dimen/dimens_20"
                    app:cardCornerRadius="@dimen/dimens_16">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="@dimen/dimens_24"
                                android:layout_height="@dimen/dimens_24"
                                android:layout_marginStart="@dimen/dimens_24"
                                android:layout_marginEnd="@dimen/dimens_8"
                                android:src="@drawable/ic_never_attended_dashboard" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/dimens_12"
                                android:fontFamily="@font/poppins_medium"
                                android:maxLines="2"
                                android:text="Never \nAttended"
                                android:textColor="@color/blackish"
                                android:textSize="@dimen/dimens_14" />
                        </LinearLayout>

                        <com.robinhood.ticker.TickerView
                            android:id="@+id/neverAttended"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="start"
                            android:layout_marginStart="@dimen/dimens_24"
                            android:layout_marginTop="@dimen/dimens_10"
                            android:layout_marginBottom="@dimen/dimens_8"
                            android:text="0"
                            android:textColor="@color/black"
                            android:textSize="@dimen/dimens_24"
                            app:ticker_animationDuration="700"
                            app:ticker_defaultPreferredScrollingDirection="up" />

                    </LinearLayout>


                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="0dp"
                    android:layout_height="140dp"
                    android:layout_marginEnd="@dimen/dimens_16"
                    android:layout_marginBottom="@dimen/dimens_2"
                    android:layout_weight="1"
                    android:elevation="@dimen/dimens_20"
                    app:cardCornerRadius="@dimen/dimens_16">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="@dimen/dimens_24"
                                android:layout_height="@dimen/dimens_24"
                                android:layout_marginStart="@dimen/dimens_24"
                                android:layout_marginEnd="@dimen/dimens_8"
                                android:src="@drawable/ic_not_picked_dashboard" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/dimens_12"
                                android:fontFamily="@font/poppins_medium"
                                android:maxLines="2"
                                android:text="Not picked \nby client"
                                android:textColor="@color/blackish"
                                android:textSize="@dimen/dimens_14" />
                        </LinearLayout>

                        <com.robinhood.ticker.TickerView
                            android:id="@+id/notPickedUpByClient"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="start"
                            android:layout_marginStart="@dimen/dimens_24"
                            android:layout_marginTop="@dimen/dimens_10"
                            android:layout_marginBottom="@dimen/dimens_8"
                            android:text="0"
                            android:textColor="@color/black"
                            android:textSize="@dimen/dimens_24"
                            app:ticker_animationDuration="700"
                            app:ticker_defaultPreferredScrollingDirection="up" />
                    </LinearLayout>


                </androidx.cardview.widget.CardView>


            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayoutSet3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dimens_8"
                android:orientation="horizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/linearLayoutSet2">

                <androidx.cardview.widget.CardView
                    android:layout_width="0dp"
                    android:layout_height="140dp"
                    android:layout_marginStart="@dimen/dimens_16"
                    android:layout_marginEnd="@dimen/dimens_16"
                    android:layout_marginBottom="@dimen/dimens_2"
                    android:layout_weight="1"
                    android:elevation="@dimen/dimens_20"
                    app:cardCornerRadius="@dimen/dimens_16">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="@dimen/dimens_24"
                                android:layout_height="@dimen/dimens_24"
                                android:layout_marginStart="@dimen/dimens_24"
                                android:layout_marginEnd="@dimen/dimens_8"
                                android:src="@drawable/ic_total_phone_dashoard" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/dimens_12"
                                android:fontFamily="@font/poppins_medium"
                                android:maxLines="2"
                                android:text="Total Phone Calls"
                                android:textColor="@color/blackish"
                                android:textSize="@dimen/dimens_14" />
                        </LinearLayout>

                        <com.robinhood.ticker.TickerView
                            android:id="@+id/totalPhoneCalls"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/dimens_10"
                            android:layout_marginBottom="@dimen/dimens_8"
                            android:text="0"
                            android:textColor="@color/black"
                            android:textSize="@dimen/dimens_24"
                            app:ticker_animationDuration="700"
                            app:ticker_defaultPreferredScrollingDirection="up" />

                    </LinearLayout>
                </androidx.cardview.widget.CardView>
            </LinearLayout>

            <TextView
                android:id="@+id/analysisText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dimens_20"
                android:layout_marginTop="@dimen/dimens_32"
                android:fontFamily="@font/poppins_medium"
                android:text="Analysis"
                android:textColor="@color/grayish"
                android:textSize="@dimen/dimens_16"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/linearLayoutSet3" />

            <LinearLayout
                android:id="@+id/linearLayoutSet4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dimens_16"
                android:orientation="horizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/analysisText">

                <androidx.cardview.widget.CardView
                    android:layout_width="0dp"
                    android:layout_height="140dp"
                    android:layout_marginStart="@dimen/dimens_16"
                    android:layout_marginEnd="@dimen/dimens_16"
                    android:layout_marginBottom="@dimen/dimens_2"
                    android:layout_weight="1"
                    android:elevation="@dimen/dimens_20"
                    app:cardCornerRadius="@dimen/dimens_16">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/dimens_36"
                            android:layout_marginTop="@dimen/dimens_12"
                            android:layout_marginEnd="@dimen/dimens_12"
                            android:fontFamily="@font/poppins_medium"
                            android:maxLines="2"
                            android:text="Top Caller"
                            android:textColor="@color/blackish"
                            android:textSize="@dimen/dimens_18" />

                        <com.robinhood.ticker.TickerView
                            android:id="@+id/topCallerName"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="start"
                            android:layout_marginStart="@dimen/dimens_36"
                            android:layout_marginTop="@dimen/dimens_8"
                            android:text="Manish Thapa"
                            android:textColor="#5F72BD"
                            android:textSize="@dimen/dimens_24"
                            app:ticker_animationDuration="700"
                            app:ticker_defaultPreferredScrollingDirection="up" />

                        <com.robinhood.ticker.TickerView
                            android:id="@+id/topCallerCallsCount"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginStart="@dimen/dimens_36"
                            android:layout_marginTop="@dimen/dimens_8"
                            android:layout_marginBottom="@dimen/dimens_8"
                            android:text="0 calls"
                            android:textColor="@color/black"
                            android:textSize="@dimen/dimens_16"
                            app:ticker_animationDuration="700"
                            app:ticker_defaultPreferredScrollingDirection="up" />
                    </LinearLayout>
                </androidx.cardview.widget.CardView>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayoutSet5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dimens_10"
                android:orientation="horizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/linearLayoutSet4">

                <androidx.cardview.widget.CardView
                    android:layout_width="0dp"
                    android:layout_height="140dp"
                    android:layout_marginStart="@dimen/dimens_16"
                    android:layout_marginEnd="@dimen/dimens_16"
                    android:layout_marginBottom="@dimen/dimens_2"
                    android:layout_weight="1"
                    android:elevation="@dimen/dimens_20"
                    app:cardCornerRadius="@dimen/dimens_16">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/dimens_36"
                            android:layout_marginTop="@dimen/dimens_12"
                            android:layout_marginEnd="@dimen/dimens_12"
                            android:fontFamily="@font/poppins_medium"
                            android:maxLines="2"
                            android:text="Total Working Hours"
                            android:textColor="@color/blackish"
                            android:textSize="@dimen/dimens_18" />

                        <com.robinhood.ticker.TickerView
                            android:id="@+id/workingTime"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="start"
                            android:layout_marginStart="@dimen/dimens_36"
                            android:layout_marginTop="@dimen/dimens_8"
                            android:text="0 hr 0 mins"
                            android:textColor="#5F72BD"
                            android:textSize="@dimen/dimens_24"
                            app:ticker_animationDuration="700"
                            app:ticker_defaultPreferredScrollingDirection="up" />

                    </LinearLayout>
                </androidx.cardview.widget.CardView>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayoutSet6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dimens_10"
                android:orientation="horizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/linearLayoutSet5">

                <androidx.cardview.widget.CardView
                    android:layout_width="0dp"
                    android:layout_height="140dp"
                    android:layout_marginStart="@dimen/dimens_16"
                    android:layout_marginEnd="@dimen/dimens_16"
                    android:layout_marginBottom="@dimen/dimens_2"
                    android:layout_weight="1"
                    android:elevation="@dimen/dimens_20"
                    app:cardCornerRadius="@dimen/dimens_16">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/dimens_36"
                            android:layout_marginTop="@dimen/dimens_12"
                            android:layout_marginEnd="@dimen/dimens_12"
                            android:fontFamily="@font/poppins_medium"
                            android:maxLines="2"
                            android:text="Total Call Duration"
                            android:textColor="@color/blackish"
                            android:textSize="@dimen/dimens_18" />

                        <com.robinhood.ticker.TickerView
                            android:id="@+id/totalCallTime"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="start"
                            android:layout_marginStart="@dimen/dimens_36"
                            android:layout_marginTop="@dimen/dimens_8"
                            android:text="0 hr 0 mins"
                            android:textColor="#5F72BD"
                            android:textSize="@dimen/dimens_24"
                            app:ticker_animationDuration="700"
                            app:ticker_defaultPreferredScrollingDirection="up" />

                    </LinearLayout>
                </androidx.cardview.widget.CardView>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayoutSet7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dimens_10"
                android:orientation="horizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/linearLayoutSet6">

                <androidx.cardview.widget.CardView
                    android:layout_width="0dp"
                    android:layout_height="140dp"
                    android:layout_marginStart="@dimen/dimens_16"
                    android:layout_marginEnd="@dimen/dimens_16"
                    android:layout_marginBottom="@dimen/dimens_2"
                    android:layout_weight="1"
                    android:elevation="@dimen/dimens_20"
                    app:cardCornerRadius="@dimen/dimens_16">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/dimens_36"
                            android:layout_marginTop="@dimen/dimens_12"
                            android:layout_marginEnd="@dimen/dimens_12"
                            android:fontFamily="@font/poppins_medium"
                            android:maxLines="2"
                            android:text="Highest Call Duration"
                            android:textColor="@color/blackish"
                            android:textSize="@dimen/dimens_18" />

                        <com.robinhood.ticker.TickerView
                            android:id="@+id/highestCallDuration"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="start"
                            android:layout_marginStart="@dimen/dimens_36"
                            android:layout_marginTop="@dimen/dimens_8"
                            android:text="0 hr 0 mins"
                            android:textColor="#5F72BD"
                            android:textSize="@dimen/dimens_24"
                            app:ticker_animationDuration="700"
                            app:ticker_defaultPreferredScrollingDirection="up" />

